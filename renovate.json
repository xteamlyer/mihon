{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  "forkProcessing": "enabled",
  "autodiscover": true,
  "branchConcurrentLimit": 0,
  "enabled": true,
  "git-submodules": {
    "enabled": true
  },
  "ignoreDeprecated": false,
  "ignoreUnstable": true,
  "includeMirrors": true,
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
}
